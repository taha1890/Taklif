grades = []

for i in range(6):
    grade = float(input(f"input your grades {i+1} "))
    grades.append(grade)

average = sum(grades) / len(grades)

print(f"your grade: {average}")

if 18 <= average <= 20:
    print("you won a ps5")
elif 16 <= average < 18:
    print("you won a bicycle")
else:
    print("sorry you a loser")
